cell c3 pin_fuel1 coolant3  1  -2   px0 -py0
cell c4 pin_fuel1 coolant4  1  -2  -px0 -py0


cell 3 0 fill pin_fuel1 -2 
cell 5 0 outside 2 


% ----- Surface for detectors
surf 3 px -0.64750000
surf 4 py 0.64750000
surf 5 px 0.64750000
surf 6 py -0.64750000

% ----- Detectors ------
det total_rate_f1  de 2groups_grid dr -1 fuel1 dc f1

