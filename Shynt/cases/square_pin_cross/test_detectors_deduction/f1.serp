det all_to_f1  de 2groups_grid dr -1 fuel1 dc f1   dfl 4 2  dfl 4 0 


det j_out_f1_thr_circle  ds 1    1 de 2groups_grid   dfl 4 3  dfl 5 1
det j_out_f1_thr_px      ds px0 -1 de 2groups_grid   dfl 4 3  dfl 5 1
det j_out_f1_thr_py      ds py0  1 de 2groups_grid   dfl 4 3  dfl 5 1


